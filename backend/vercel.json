{
    "version": 2,
    "functions": {
        "server.js": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "server.js"
        },
        {
            "source": "/(.*)",
            "destination": "server.js"
        }
    ]
}
